@model postcard.Models.Home
<div class="row">
    @foreach (var item in Model.listcapas)
    {
        <div class="col">
            <div class="card cardcapa">
                <img src="images/@item.imagem" class="logopostcard" alt="...">
                <div class="card-body">
                    <h5 class="card-title">@item.uf</h5>
                    <p class="card-text">@item.musica<a href="@item.youtube" target="_blank">mais</a></p>
                    <a href="#" class="btn btn-primary">Ver cartões</a>
                </div>   
            </div>
        </div>
     }
 </div>