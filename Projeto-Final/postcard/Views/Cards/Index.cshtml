@model IEnumerable<postcard.Models.PostCards>
<div class="container text-center">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col">
                <div class="card cardcapa">
                    <img src="images/cards/@item.uf/@item.card" class="logopostcard" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">@item.cidade - @item.descricao</h5>
                        <p class="card-text"></p>
                        <a href='#' class="btn btn-primary">Download</a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>